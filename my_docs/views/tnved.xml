<odoo>
  <data>
    <record id="view_tn_ved" model="ir.ui.view">
      <field name="name">my_docs.tnved.tree</field>
      <field name="model">my_docs.tnved</field>
      <field name="priority" eval="8" />
      <field name="arch" type="xml">
          <tree string="TnVed Sheet">
            <field name="name" />
            <field name="note" />
          </tree>
      </field>
    </record>

    <record id="tn_ved_view_search" model="ir.ui.view">
      <field name="name">my_docs.tnved.tree.search</field>
        <field name="model">my_docs.tnved</field>
        <field name="arch" type="xml">
            <search string="Search">
              <field name="name" />
              <field name="note" />
           </search>
       </field>
    </record>

    <record id="view_tn_ved_form" model="ir.ui.view">
        <field name="name">my_docs.tnved.form</field>
        <field name="model">my_docs.tnved</field>
        <field name="priority" eval="8" />
        <field name="arch" type="xml">
            <form string="Tn Ved">
                <sheet>
                  <group col="4">
                    <field name="name" colspan="4"/>
                  </group>
                  <group col="4">
                    <field name="note" colspan="4"/>
                  </group>
                </sheet>
              </form>
            </field>
        </record>
  </data>
</odoo>
